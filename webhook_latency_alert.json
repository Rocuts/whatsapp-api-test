{
  "displayName": "Webhook Latency Alert",
  "combiner": "OR",
  "conditions": [
    {
      "displayName": "p95 latency > 800ms",
      "conditionThreshold": {
        "filter": "metric.type=\"logging.googleapis.com/user/webhook_latency_ms\" resource.type=\"cloud_run_revision\"",
        "comparison": "COMPARISON_GT",
        "thresholdValue": 800,
        "duration": "300s",
        "trigger": {
          "count": 1
        },
        "aggregations": [
          {
            "alignmentPeriod": "60s",
            "perSeriesAligner": "ALIGN_PERCENTILE_95"
          }
        ]
      }
    }
  ]
}